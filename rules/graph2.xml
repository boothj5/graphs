<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
     http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
  <key attr.name="type" attr.type="string" for="node" id="type"/>
  <graph id="graph" edgedefault="directed">
    <node id="prop1">
        <data key="type">property</data>
    </node>
    <node id="int1">
        <data key="type">interim</data>
    </node>
    <node id="int2">
        <data key="type">interim</data>
    </node>
    <edge id="edge1" source="prop1" target="int1"/>
    <edge id="edge2" source="prop1" target="int2"/>
  </graph>
</graphml>